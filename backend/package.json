    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "1.4.5-lts.1",
    "multer-s3": "2.10.0",
    "openai": "^4.20.1",
    "pdf-parse": "^1.1.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.27",
    "uuid": "^13.0.0"
